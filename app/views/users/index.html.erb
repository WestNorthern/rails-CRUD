<h1>Users#index</h1>
<% @users.each do |user| %>

<div class="user">
	<p><%= user.first_name %> <%= user.last_name %> <%= user.email %></p>
	<%= link_to "Users Show", user %>
	<%= link_to "Users Show #{user.full_name}", user %>
	<%= link_to "Edit", edit_user_path(user) %>
	<%= link_to "Destroy", user, method: :delete %>
	<%= link_to "New Post", new_post_path(user_id: user.id) %>
</div>

<% end %>

